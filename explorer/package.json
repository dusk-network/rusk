    "eslint-import-resolver-custom-alias": "1.3.2",
    "eslint-plugin-svelte": "2.46.1",
    "jsdom": "26.1.0",
    "jsdom-worker": "0.3.0",
    "lamb-types": "0.61.13",
    "postcss-nested": "7.0.2",
    "prettier": "3.5.3",
    "prettier-plugin-svelte": "3.3.3",
    "svelte": "4.2.19",
